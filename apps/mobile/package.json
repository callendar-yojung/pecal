{
  "name": "mobile-app",
  "private": true,
  "main": "expo-router/entry",
  "scripts": {
    "start": "EXPO_NO_WATCHMAN=1 expo start --clear",
    "android": "EXPO_NO_WATCHMAN=1 expo start --android --clear",
    "ios": "EXPO_NO_WATCHMAN=1 expo start --ios --clear",
    "web": "EXPO_NO_WATCHMAN=1 expo start --web --clear",
    "reset-cache": "rm -rf .expo && rm -rf /tmp/metro-* /tmp/haste-map-*"
  },
  "dependencies": {
    "@expo/metro-runtime": "^6.1.2",
    "@expo/vector-icons": "^15.0.3",
    "@react-native-async-storage/async-storage": "^2.2.0",
    "@repo/api-client": "workspace:*",
    "expo": "^54.0.0",
    "expo-linking": "~8.0.8",
    "expo-router": "^6.0.23",
    "expo-status-bar": "~3.0.9",
    "expo-web-browser": "~15.0.8",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-native": "0.81.5",
    "react-native-safe-area-context": "^5.6.2",
    "react-native-webview": "^13.15.0",
    "react-native-web": "0.21.0"
  },
  "devDependencies": {
    "@babel/core": "^7.25.2",
    "@types/node": "^20.19.24",
    "@types/react": "^19.1.17",
    "@types/react-dom": "^19.1.11",
    "typescript": "~5.9.3"
  }
